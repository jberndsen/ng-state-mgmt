{
  "name": "seed",
  "description": "Angular state management demo project, powered by Angular CLI",
  "author": "Jeroen Berndsen",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "Angular",
    "State management"
  ],
  "scripts": {
    "checks": "npm run lint",
    "clean": "rimraf dist coverage node_modules tmp && npm cache clean",
    "clean-install": "npm run clean && npm install",
    "ng": "ng",
    "check-updates": "ncu",
    "start": "ng serve --proxy-config proxy.conf.json",
    "prebuild": "npm run checks",
    "build": "rimraf dist && ng build --prod --aot --base-href ./ && ncp deploy/. dist",
    "test": "ng test --single-run --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^4.1.0",
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/material": "^2.0.0-beta.5",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/router": "^4.1.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.2",
    "@ngrx/store": "^2.2.1",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "moment": "^2.18.1",
    "ng-click-outside": "^2.0.2",
    "normalize.css": "^6.0.0",
    "rxjs": "^5.1.0",
    "web-animations-js": "^2.2.2",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.1",
    "@angular/compiler-cli": "^4.1.0",
    "@angular/language-service": "^4.1.0",
    "@ngrx/store-devtools": "^3.2.4",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.58",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ncp": "^2.0.0",
    "npm-check-updates": "^2.10.4",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.3.0"
  }
}
